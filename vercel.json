{
  "version": 2,
  "builds": [
      {
          "src": "build/app.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["build/**"] }
      }
  ],
  "buildCommand": "npm run build",
  "devCommand": "npm run start:dev",
  "routes": [
      {
          "src": "/(.*)",
          "dest": "build/app.js"
      }
  ]
}